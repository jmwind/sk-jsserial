    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Metrics",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "--metrics"
            ],
            "program": "${workspaceFolder}/cli.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Polars",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "--polars",
                "data/catalina36_polar.txt",
                "12.1",
                "87"
            ],
            "program": "${workspaceFolder}/cli.js"
        }
    ]
}